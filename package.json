{
  "name": "@open-formulieren/admin-ui",
  "version": "0.1.0",
  "description": "The new UI for Open Forms administrators.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "build:typecheck": "tsc --noEmit",
    "test": "TZ=Europe/Amsterdam npm-run-all test:*",
    "test:storybook": "TZ=Europe/Amsterdam vitest --run --project=storybook",
    "test:vitest": "TZ=Europe/Amsterdam vitest --run --project=unit",
    "format": "prettier --write 'src/**/*' '.storybook/**/*'",
    "checkformat": "prettier --check 'src/**/*' '.storybook/**/*'",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "makemessages": "formatjs extract",
    "compilemessages": "formatjs compile-folder --ast i18n/messages i18n/compiled",
    "lint": "eslint src"
  },
  "files": [
    "dist/",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/open-formulieren/admin-ui.git"
  },
  "author": "Maykin Media",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/open-formulieren/admin-ui/issues"
  },
  "homepage": "https://github.com/open-formulieren/admin-ui#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-intl": "^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.10.0",
    "@maykinmedia/eslint-config": "^2.0.0",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-formatjs": "^11.1.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-storybook": "^10.1.11",
    "msw": "^2.12.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.0.0",
    "sass-embedded": "^1.97.3",
    "storybook": "^10.1.11",
    "storybook-addon-remix-react-router": "^6.0.0",
    "storybook-react-intl": "^10.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "vitest-browser-react": "^2.0.2"
